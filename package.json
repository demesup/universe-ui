{
  "name": "autoparter-ui",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "license": "SEE LICENSE IN LICENSE.md",
  "packageManager": "pnpm@8.14.0",
  "author": {
    "name": "demesup"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=10"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -e -V "
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix"
    ],
    "*.scss": [
      "stylelint"
    ],
    "*.md": [
      "eslint --fix"
    ],
    "*.vue": [
      "eslint --fix"
    ]
  },
  "scripts": {
    "dev": "run-p dev:vite",
    "dev:vite": "vite",
    "build": "run-s build:*",
    "build:update-bulma-colors": "bulma-css-vars",
    "build:vite": "vite build",
    "ssr:build": "run-s build:update-bulma-colors ssr:build:*",
    "ssr:build:client": "cross-env NODE_ENV=production vite build --ssrManifest --outDir dist/client",
    "ssr:build:server": "cross-env NODE_ENV=production vite build --ssr src/entry-server.ts --outDir dist/server",
    "ssr:dev": "jiti server/serve.ts",
    "ssr:serve": "cross-env NODE_ENV=production jiti server/serve.ts",
    "ssr:preview": "run-p ssr:serve preview:json-server",
    "ssg:build": "pnpm build:update-bulma-colors && cross-env NODE_ENV=production jiti server/generate.ts",
    "ssg:serve": "serve dist -p 3000",
    "preview": "run-p preview:vite preview:json-server",
    "preview:vite": "serve dist -s -p 5000",
    "preview:json-server": "json-server -q --read-only --routes ./json-server/routes.json --host 0.0.0.0 --port 8080 ./json-server/db.json",
    "lint": "run-s lint:eslint:fix:* lint:stylelint:fix",
    "lint:eslint": "eslint ./src",
    "lint:eslint:fix": "eslint --fix ./src",
    "lint:eslint:fix:doc": "eslint --fix ./documentation",
    "lint:stylelint": "stylelint ./src/**/*.{vue,css,scss}",
    "lint:stylelint:fix": "stylelint ./src/**/*.{vue,css,scss} --fix",
    "test": "run-p test:lint test:tsc test:e2e",
    "test:tsc": "vue-tsc --noEmit",
    "test:lint": "run-p lint:eslint lint:stylelint",
    "test:e2e": "cypress run",
    "test:unit": "echo \"[warning] no unit test implemented yet\"",
    "cypress": "cypress"
  },
  "dependencies": {
    "@ckeditor/ckeditor5-build-classic": "41.0.0",
    "@ckeditor/ckeditor5-vue": "5.1.0",
    "@cssninja/bulma-css-vars": "0.9.2",
    "@iconify/iconify": "3.1.1",
    "@mapbox/mapbox-gl-geocoder": "5.0.2",
    "@minify-html/node": "^0.15.0",
    "@nuxt/devalue": "2.0.2",
    "@popperjs/core": "2.11.8",
    "@unhead/addons": "1.8.10",
    "@unhead/ssr": "1.8.10",
    "@unhead/vue": "1.8.10",
    "@vee-validate/zod": "4.12.4",
    "@vueform/multiselect": "2.6.6",
    "@vueform/slider": "2.1.10",
    "@vueuse/core": "10.7.2",
    "@vueuse/router": "10.7.2",
    "apexcharts": "3.45.2",
    "billboard.js": "3.10.3",
    "bulma": "npm:@cssninja/bulma@0.9.4",
    "compression": "1.7.4",
    "dayjs": "1.11.10",
    "defu": "6.1.4",
    "dragula": "3.7.3",
    "dropzone": "6.0.0-beta.2",
    "filepond": "4.30.6",
    "filepond-plugin-file-validate-size": "2.2.8",
    "filepond-plugin-file-validate-type": "1.2.8",
    "filepond-plugin-image-crop": "2.0.6",
    "filepond-plugin-image-edit": "1.6.3",
    "filepond-plugin-image-exif-orientation": "1.0.11",
    "filepond-plugin-image-preview": "4.6.12",
    "filepond-plugin-image-resize": "2.0.10",
    "filepond-plugin-image-transform": "3.8.7",
    "focus-trap": "7.5.4",
    "focus-trap-vue": "4.0.3",
    "h3": "1.10.0",
    "html2pdf.js": "0.9.3",
