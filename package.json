    "unplugin-vue-router": "0.7.0",
    "v-calendar": "3.0.3",
    "vee-validate": "4.12.4",
    "vivus": "0.4.6",
    "vue": "3.4.15",
    "vue-accessible-color-picker": "5.0.1",
    "vue-i18n": "9.9.0",
    "vue-router": "4.2.5",
    "vue-scrollto": "2.20.0",
    "vue-tippy": "6.4.1",
    "vue3-apexcharts": "1.4.4",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@ckeditor/ckeditor5-font": "^41.0.0",
