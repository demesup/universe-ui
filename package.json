    "vite-plugin-pwa": "0.17.4",
    "vite-plugin-radar": "0.9.2",
    "vue-component-meta": "^1.8.27",
    "vue-tsc": "1.8.27",
    "yorkie": "2.0.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "postcss",
        "react-dom",
        "react"
      ],
      "allowedVersions": {
        "mapbox-gl": "3.0.0"
      }
    },
    "patchedDependencies": {
      "@purge-icons/core@0.10.0": "patches/@purge-icons__core@0.10.0.patch"
    }
  }
